{
  "name": "kong-manager",
  "private": true,
  "version": "0.0.0.0",
  "type": "module",
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "install:e2e": "yarn --cwd tests/playwright install",
    "test:e2e": "yarn --cwd tests/playwright run test",
    "playwright": "yarn --cwd tests/playwright run playwright",
    "preview": "vite build && vite preview",
    "lint": "yarn lint:eslint && yarn lint:vue && yarn lint:playwright && yarn lint:style",
    "lint:eslint": "eslint src tests/playwright",
    "lint:eslint:fix": "eslint --fix src tests/playwright",
    "lint:playwright": "tsc --noEmit --project tests/playwright/tsconfig.json --baseUrl tests/playwright",
    "lint:style": "stylelint --allow-empty-input 'src/**/*.{css,scss,sass,less,styl,vue}'",
    "lint:vue": "vue-tsc --noEmit",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,js,vue}": "eslint --fix",
    "src/**/*.{css,scss,sass,less,styl,vue}": "stylelint --allow-empty-input --fix 'src/**/*.{css,scss,sass,less,styl,vue}'"
  },
  "dependencies": {
    "@kong-ui-public/app-layout": "^4.2.18",
    "@kong-ui-public/entities-certificates": "^3.4.0",
    "@kong-ui-public/entities-consumer-credentials": "^3.2.23",
    "@kong-ui-public/entities-consumers": "^3.3.11",
    "@kong-ui-public/entities-data-plane-nodes": "^0.2.22",
    "@kong-ui-public/entities-gateway-services": "^3.5.11",
    "@kong-ui-public/entities-key-sets": "^3.3.11",
    "@kong-ui-public/entities-keys": "^3.3.11",
    "@kong-ui-public/entities-plugins": "^8.3.0",
    "@kong-ui-public/entities-routes": "^3.2.0",
    "@kong-ui-public/entities-shared": "^3.2.0",
    "@kong-ui-public/entities-snis": "^3.3.11",
    "@kong-ui-public/entities-upstreams-targets": "^3.3.15",
    "@kong-ui-public/entities-vaults": "^3.8.3",
    "@kong-ui-public/forms": "^4.0.2",
    "@kong-ui-public/i18n": "^2.2.2",
    "@kong-ui-public/misc-widgets": "^2.1.11",
    "@kong/icons": "^1.14.0",
    "@kong/kongponents": "^9.1.7",
    "@material-design-icons/font": "^0.14.9",
    "axios": "^1.6.0",
    "dayjs": "^1.11.10",
    "marked": "^5.1.0",
    "monaco-editor": "0.21.3",
    "pinia": "^2.1.6",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@kong/design-tokens": "^1.15.3",
    "@kong/eslint-config-kong-ui": "^1.1.1",
    "@playwright/test": "^1.46.0",
    "@types/lodash-es": "^4.17.12",
    "@types/marked": "^5.0.1",
    "@types/node": "^20.3.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.14",
    "bmp-js": "^0.1.0",
    "dotenv": "^16.0.1",
    "eslint": "^9.5.0",
    "eslint-plugin-playwright": "^0.18.0",
    "eslint-plugin-vuejs-accessibility": "^2.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "lodash-es": "^4.17.21",
    "postcss-html": "^1.5.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.63.4",
    "stylelint": "^15.10.2",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "typescript": "^5.3.2",
    "vite": "^5.2.11",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vue-tsc": "^2.0.16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not IE <= 11"
  ]
}
